#!/usr/bin/make -f

CFLAGS = -Wall -g

ifneq (,$(findstring noopt,$(DEB_BUILD_OPTIONS)))
        CFLAGS += -O0
else
        CFLAGS += -O2
endif

%:
	dh $@

override_dh_clean:
	rm -f nxcomp/changelog
	rm -f nxproxy/changelog
	dh_clean -X autom4te.cache

override_dh_auto_build:
	debian/pre-build-nx.sh
	dh_auto_build
	debian/post-build-nx.sh

override_dh_auto_install:


